{
  "version": 2,
  "builds": [
    {
      "src": "UrbanInfra/backend/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "UrbanInfra/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "UrbanInfra/backend/app.py" },
    { "src": "/(.*)", "dest": "UrbanInfra/index.html" }
  ]
}
